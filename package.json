    "@supabase/auth-ui-shared": "^0.1.8",
    "@supabase/ssr": "^0.7.0",
    "@supabase/supabase-js": "^2.81.1",
    "@types/uuid": "^10.0.0",
    "@xenova/transformers": "^2.17.2",
    "axios": "^1.13.2",
    "chart.js": "^4.5.1",
    "chartjs-adapter-date-fns": "^3.0.0",
    "date-fns": "^4.1.0",
    "groq-sdk": "^0.36.0",
    "next": "16.0.1",
    "node-cron": "^4.2.1",
    "openai": "^6.8.1",
    "pg": "^8.16.3",
    "react": "19.2.0",
    "react-chartjs-2": "^5.3.1",
    "react-dom": "19.2.0",
    "react-icons": "^5.5.0",
