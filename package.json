{
  "name": "ai-news-sentiment-analyzer",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev -p 3001",
    "build": "next build",
    "start": "next start",
    "lint": "eslint . --ext .ts,.tsx --fix",
    "format": "prettier --write .",
    "collector": "ts-node -P tsconfig.scripts.json scripts/background-collector-clean.ts",
